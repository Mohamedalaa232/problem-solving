#include <iostream>
#include <cmath>
using namespace std;
 
int main() {
    double A, B;
    cin >> A >> B;
 
    double result = A / B;
 
    cout << "floor " << (int)A << " / " << (int)B << " = " << floor(result) << endl;
    cout << "ceil " << (int)A << " / " << (int)B << " = " << ceil(result) << endl;
    cout << "round " << (int)A << " / " << (int)B << " = " << round(result) << endl;
 
    return 0;
}
